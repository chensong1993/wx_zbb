  border-top: 12rpx solid #ececec;
  
}
.hangye-header{
padding-left: 10rpx;
border-left: 10rpx solid #4093ff;
font-size: 32rpx;
font-weight: bold;
} 
.hangye-main .next{
  float: right;
  width: 40rpx;
  height: 40rpx;
  margin-top: 15rpx;
  transform: rotate(180deg);
}

.hangye-category{
  margin: 0 20rpx;
  font-size: 26rpx;
  line-height: 60rpx;
}
.hangye-title{
  font-size: 30rpx;
  font-weight: bold;
}

.hangye-num{
  float: right;
