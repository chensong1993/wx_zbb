@import "weui.wxss";

.data-main {
  
}


.data-distribution {
  overflow: hidden;
  clear: both;
  border-top: 12rpx solid #ececec;
  margin-bottom: 20rpx;
}

.data-distribution text {
  display: block;
  text-align: center;
  margin: 20rpx 30rpx;
  height: 25rpx;
  font-size: 26rpx;
}

.diqu {
  font-weight: bold;
}

.percent {
  color: #d71820;
}

.fenbu {
  font-size: 30rpx;
  padding-bottom: 30rpx;
  border-bottom: 2rpx solid #ececec;
}

.diqu-distribution {
  float: left;
  left: 0;
  width: 50%;
}

.hangye-distribution {
  float: right;
  right: 0;
  width: 50%;
}
.data-distribution .num{
  color: #959595;
}
.guapai-mains {
  border-top: 12rpx solid #ececec;
}

.guapai-gongsi {
  padding: 10rpx 20rpx;
  line-height: 65rpx;
  border-bottom: 1rpx solid #ececec;
  font-size: 32rpx;
}

.guapai-gongsi .gongsi {
  border-left: 10rpx solid #4093ff;
  padding-left: 10rpx;
  font-weight: bold;
   
}

.guapai-gongsi .date {
  float: right;
}
.guapai-main{
  overflow: hidden;
  clear: both;
  line-height: 70rpx;
  padding: 0 20rpx;
  font-size: 26rpx;
  border-bottom: 1rpx solid #ececec;
}
.guapai-main .guapai-name{
  float: left;

}
.guapai-main .guapai-num{
  float: right;
  color: #d71820;
}

