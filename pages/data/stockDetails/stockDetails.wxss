  font-size: 30rpx;
}

.price {
  margin-bottom: 10rpx;
  font-weight: bold;
  font-size: 28rpx;
}

.zhangdie {
  border-bottom: 1rpx solid gainsboro;
  font-weight: bold;
  height: 60rpx;
  line-height: 60rpx;
}

.zhangdie .num {
  margin-right: 20rpx;
  margin-bottom: 20rpx;
}
.deal_type{
  float: right;
  border: 2rpx solid #4093ff;
  color: #4093ff;
  border-radius: 15rpx;
  padding:5rpx 10rpx;
}
.is_innovate{
   float: right;
   margin-left: 20rpx;
  border: 2rpx solid darkviolet;
  color: darkviolet;
  border-radius: 15rpx;
  padding:5rpx 10rpx;
}
.data-main {
  margin-top: 10rpx;
  clear: both;
  overflow: hidden;
  line-height: 40rpx;
  font-size: 22rpx;
}

.left-content {
  width: 33.3%;
  float: left;
}

.left-content1 {
}

.left-content2 {
  float: right;
  padding-right: 20rpx;
}

.center-content {
  width: 33.3%;
  float: right;
}

.center-content1 {
}

.center-content2 {
  float: right;
  padding-right: 20rpx;
}

.right-content {
  width: 33.3%;
  float: right;
}

.right-content1 {
}

.right-content2 {
  float: right;
}

.no_data {
  width: 100%;
  text-align: center;
  line-height: 500rpx;
}

.openApp_V {
  width: 100%;
  text-align: center;
  margin-top: 50rpx;
}

.openApp {
  height: 80rpx;
  width: 200rpx;
  color: white;
  background: #4093ff;
  padding: 20rpx;
  border-radius: 20rpx;
}

.ranking_title .text.active {
  border-bottom: 5rpx solid #4093ff;
}

.ranking_title {
  height: 80rpx;
  line-height: 80rpx;
  width: 100%;
  font-size: 30rpx;
  border-top: 1rpx solid #ececec;
  border-bottom: 1rpx solid #ececec;
}

.ranking_title text {
  width: 33.3%;
  display: inline-block;
  text-align: center;
}

